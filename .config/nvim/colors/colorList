" Maintainer: Nil Ventosa  <ventosa@gmail.com>

set background=dark
highlight clear
if exists("syntax_on")
    syntax reset
endif

" Colors
let g:colors_name = "test"

let g:tucnak#palette =              {}
let g:tucnak#palette.fg =           ['#ffffff']
let g:tucnak#palette.bg =           ['#011627']
let g:tucnak#palette.cyan =         ['#8BE9FD']
let g:tucnak#palette.magenta =      ['#ff2d7d']
let g:tucnak#palette.black =        ['#282828']
let g:tucnak#palette.darkblue =     ['#011627']
let g:tucnak#palette.white =        ['#ffffff']
let g:tucnak#palette.yellow =       ['#f7f300']


let s:bg        = g:tucnak#palette.bg
let s:fg        = g:tucnak#palette.fg
let s:cyan      = g:tucnak#palette.cyan
let s:magenta   = g:tucnak#palette.magenta
let s:black     = g:tucnak#palette.black
let s:darkblue  = g:tucnak#palette.darkblue
let s:yellow    = g:tucnak#palette.yellow
let s:white     = g:tucnak#palette.white
let s:none      = ['NONE', 'NONE']

" Highlight groups
call s:h('tucnakMagenta', s:magenta)

" Vim
hi! link vimLineComment tucnakMagenta
